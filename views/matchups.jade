extends layouts/listing

block listing-table

	table.table.table-hover
		thead
			th Status
			th Team A
			th Team B
			th Best Of
			th Played
			th Action

		tbody#eventsTable

block append scripts
	script(src='/javascripts/vendor/moment/moment.min.js')
	script
		$(function() {
			var Collection = Koala.collections.new('#{id});
			var View = Koala.views.new('matchup_tbody', {
				collection: Collection
			});

			Collection.fetch({
				data: location.search.replace(/^\?(.*)/gi, '$1')
			});
		});


